{
  "name": "timesync",
  "version": "0.0.1",
  "description": "Time synchronization between peers",
  "author": "Jos de Jong <wjosdejong@gmail.com> (https://github.com/josdejong)",
  "main": "./timesync.js",
  "keywords": [
    "time",
    "synchronization",
    "ntp",
    "client",
    "server",
    "isomorphic"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/enmasseio/timesync.git"
  },
  "dependencies": {
    "pubnub": "^3.7.8",
    "socket.io": "^1.3.2"
  },
  "devDependencies": {
    "6to5": "^3.0.15",
    "uglify-js": "^2.4.16",
    "watch": "^0.13.0"
  },
  "scripts": {
    "6to5": "6to5 lib/timesync.es6.js -o timesync.js",
    "minify": "uglifyjs timesync.js -o timesync.min.js",
    "build": "npm run 6to5; npm run minify",
    "watch": "watch \"npm run build\" lib"
  }
}
